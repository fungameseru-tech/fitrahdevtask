{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/admin.html",
      "dest": "/Frontend/admin.html"
    },
    {
      "src": "/script.js",
      "dest": "/Frontend/script.js"
    },
    {
      "src": "/(.*\\.(css|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/Frontend/$1"
    },
    {
      "src": "/",
      "dest": "/Frontend/index.html"
    }
  ]
}
